{
  "name": "perfrunner-reporters",
  "version": "0.1.0",
  "description": "Home of the perfrunner reporters",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "exit 0",
    "prettier": "prettier --config ../../.prettierrc --write ./src/**/*.ts",
    "tsc": "tsc",
    "copyfiles": "copyfiles",
    "copy:defaultReporter": "copyfiles src/default/index.html dist/default",
    "parcel": "parcel",
    "build:reporter": "parcel build src/reporters/default/index.html --no-source-maps --out-dir dist/reporters/default",
    "build": "tsc -p . && npm run build:reporter",
    "pack": "npm pack"
  },
  "keywords": [],
  "author": "drag13",
  "readme": "https://github.com/Drag13/perfrunner/blob/master/packages/perfrunner-reporters/readme.md",
  "homepage": "https://github.com/Drag13/perfrunner",
  "license": "MIT",
  "devDependencies": {
    "@types/chart.js": "2.9.21",
    "@types/json2csv": "5.0.1",
    "@types/mustache": "4.0.1",
    "@types/node": "14.0.1",
    "@types/web-resource-inliner": "4.2.0",
    "copyfiles": "2.2.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-inline-source": "1.0.0",
    "prettier": "2.0.5",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "chart.js": "2.9.3",
    "json2csv": "5.0.1",
    "mustache": "4.0.1",
    "perfrunner-core": "^0.1.0"
  },
  "files": [
    "dist"
  ]
